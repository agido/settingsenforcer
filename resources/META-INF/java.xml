<idea-plugin>

    <extensions defaultExtensionNs="com.agido.idea.settings">
        <Type implementation="com.agido.idea.settings.plugins.java.compiler.BackendCompilerType"/>

        <Group implementation="com.agido.idea.settings.plugins.java.compiler.JavaCompilerGroup"/>
        <Setting implementation="com.agido.idea.settings.plugins.java.compiler.BuildProcessVMOptionsSetting"/>
        <Setting implementation="com.agido.idea.settings.plugins.java.compiler.DefaultCompilerSetting"/>
        <Setting implementation="com.agido.idea.settings.plugins.java.compiler.ProjectBytecodeTargetSetting"/>

        <Group implementation="com.agido.idea.settings.plugins.java.eclipse.EclipseCompilerGroup"/>
        <Setting implementation="com.agido.idea.settings.plugins.java.eclipse.DeprecationSetting"/>
        <Setting implementation="com.agido.idea.settings.plugins.java.eclipse.GenerateNoWarningsSetting"/>
    </extensions>

</idea-plugin>
